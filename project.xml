<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="BambaMagicKeep" package="com.sample.bambamagickeep" version="1.0.0" company="Company Name" />
	<app main="BambaMain" path="Export" file="BambaMagicKeep" />
	
	<window width="1280" height="720"/>
	<source path="Source" />
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="swf" />
	<haxelib name="tjson"/>
	<assets path="Assets" rename="assets" />
	<assets path="assets/swf/IntroBamba.swf" embed="false" id="IntroBamba" />
	<assets path="assets/swf/OpeningAssets.swf" preload="true" generate="true"  />
	<assets path="assets/openingAssets/openingAssets.zip" preload="true" id="openingAssets"/> 
	<assets path="assets/xmls/params.xml" embed="false" id="params" />
	<assets path="assets/xmls/dictionary.xml" embed="false" id="dictionary" />
	<assets path="assets/xmls/generalData.xml" embed="false" id="generalData" />
	<assets path="assets/json/generalData.json" embed="false" id="generalDataJson" />
	<assets path="assets/xmls/playerLevels.xml" embed="false" id="playerLevels" />
	<assets path="assets/xmls/magicBook.xml" embed="false" id="magicBook" />
	<assets path="assets/json/magicBook.json" embed="false" id="magicBookJson" />
	<assets path="assets/fonts/Arial.ttf" id="Arial" embed="true" />
	<assets path="assets/fonts/Choco.ttf" id="Choco" embed="true" />
	<assets path="assets/fonts/Choco_Bold.ttf" id="Choco Bold" embed="true" />
</project>